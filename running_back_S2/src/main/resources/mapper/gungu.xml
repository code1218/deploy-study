<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.korit.running_back_s2.domain.gungu.GunguMapper">

    <select id="findGunguIdByName" resultType="java.lang.Integer">
        select
            gungu_id
        from
            gungu_tb
        where
            gungu_name = #{gunguName}
    </select>

    <select id="getAllGungu" resultType="com.korit.running_back_s2.domain.gungu.Gungu">
        select
            gungu_id,
            gungu_name
        from
            gungu_tb
    </select>

</mapper>
